package sbs.milpo.model.dal;


public class Geolitologia {

    private String id = "";
    private String codigo_GeoLitologia = "";
    private String codigoEmpresa = "";
    private String codigoUnidadMinera = "";
    private String litologia = "";
    private String litologiaDesc = "";
    private String estado = "";
    
    
    
	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(String id) {
		this.id = id;
	}
	
	
	
	/**
	 * @return the codigo_GeoLitologia
	 */
	public String getCodigo_GeoLitologia() {
		return codigo_GeoLitologia;
	}
	/**
	 * @param codigo_GeoLitologia the codigo_GeoLitologia to set
	 */
	public void setCodigo_GeoLitologia(String codigo_GeoLitologia) {
		this.codigo_GeoLitologia = codigo_GeoLitologia;
	}
	/**
	 * @return the estado
	 */
	public String getEstado() {
		return estado;
	}
	/**
	 * @param estado the estado to set
	 */
	public void setEstado(String estado) {
		this.estado = estado;
	}
	/**
	 * @return the litologiaDesc
	 */
	public String getLitologiaDesc() {
		return litologiaDesc;
	}
	/**
	 * @param litologiaDesc the litologiaDesc to set
	 */
	public void setLitologiaDesc(String litologiaDesc) {
		this.litologiaDesc = litologiaDesc;
	}
	/**
	 * @return the codigoEmpresa
	 */
	public String getCodigoEmpresa() {
		return codigoEmpresa;
	}
	/**
	 * @param codigoEmpresa the codigoEmpresa to set
	 */
	public void setCodigoEmpresa(String codigoEmpresa) {
		this.codigoEmpresa = codigoEmpresa;
	}
	/**
	 * @return the codigoUnidadMinera
	 */
	public String getCodigoUnidadMinera() {
		return codigoUnidadMinera;
	}
	/**
	 * @param codigoUnidadMinera the codigoUnidadMinera to set
	 */
	public void setCodigoUnidadMinera(String codigoUnidadMinera) {
		this.codigoUnidadMinera = codigoUnidadMinera;
	}
	/**
	 * @return the litologia
	 */
	public String getLitologia() {
		return litologia;
	}
	/**
	 * @param litologia the litologia to set
	 */
	public void setLitologia(String litologia) {
		this.litologia = litologia;
	}
    
    

    
}
